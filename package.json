{
  "name": "@abidibo/react-cam-roi",
  "version": "0.0.14",
  "description": "A react component for drawing ROI over images and managing metadata",
  "repository": {
    "type": "git",
    "url": "https://github.com/abidibo/react-cam-roi"
  },
  "bugs": {
    "url": "https://github.com/abidibo/react-cam-roi/issues"
  },
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/types/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "directories": {
    "test": "tests"
  },
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/types/index.d.ts",
      "style": "./dist/index.esm.css"
    },
    "./dist/index.esm.css": "./dist/index.esm.css"
  },
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "rollup -c",
    "postbuild": "npm run copy-files",
    "copy-files": "copyfiles -u 1 src/**/*.module.css dist/",
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build -o docs"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md"
          ]
        }
      ],
      "@semantic-release/npm"
    ]
  },
  "author": "abidibo",
  "license": "MIT",
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.3",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@eslint/js": "^9.18.0",
    "@fontsource/roboto": "^5.1.1",
    "@mui/material": "^6.4.3",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@storybook/addon-essentials": "^8.4.7",
    "@storybook/addon-interactions": "^8.4.7",
    "@storybook/addon-onboarding": "^8.4.7",
    "@storybook/addon-toolbars": "^8.5.0",
    "@storybook/blocks": "^8.4.7",
    "@storybook/react": "^8.4.7",
    "@storybook/react-vite": "^8.4.7",
    "@storybook/test": "^8.4.7",
    "@trivago/prettier-plugin-sort-imports": "^5.2.1",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "copyfiles": "^2.4.1",
    "eslint": "^9.18.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "rollup": "^4.40.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "semantic-release": "^24.2.2",
    "storybook": "^8.4.7",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "dependencies": {
    "fabric": "^6.5.4",
    "uuidv4": "^6.2.13",
    "vite": "^6.1.0"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  }
}
